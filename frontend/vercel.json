{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/templates/index.html"
    },
    {
      "src": "/logs",
      "dest": "/templates/logs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/index.html"
    }
  ]
}
